<div class="list-group">
    <% @tweets.each do |tweet| %>
    <div class="list-group-item">
        <div class="well">
            <small class="muted">
                <em>
                    <%= time_ago_in_words tweet.created_at %>
                    ago
                    <%= tweet.user.username %>
                    Tweeted:
                    <br>
                </em>
            </small>
            <%= tweet.body %>
            <%= link_to tweet_path(id: tweet.id), method: 'delete' do %>
            <span class="glyphicon glyphicon-trash"></span>
            <% end %>
        </div>
    </div>
    <% end %>

</div>
